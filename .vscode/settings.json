{
    "typescript.check.workspaceVersion": false,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "**/*.js": {"when": "$(basename).ts"},
        "**/*?.js": {"when": "$(basename).tsx"},
        "**/*.js.map": {"when": "$(basename)"}
    }
}